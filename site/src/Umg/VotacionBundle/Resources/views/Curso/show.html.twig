{% extends '::base.html.twig' %}
{% block titulo %}
    <h1 class="page-title">Curso</h1>
{% endblock %}

{% block body -%}
    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Curso</th>
