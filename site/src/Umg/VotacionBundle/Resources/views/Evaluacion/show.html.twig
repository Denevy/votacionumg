{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Evaluacion</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Activa</th>
                <td>{{ entity.Activa }}</td>
            </tr>
            <tr>
                <th>Fechahora</th>
                <td>{{ entity.FechaHora|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Fechahorafinal</th>
                <td>{{ entity.FechaHoraFinal|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Campuscarrera_id</th>
                <td>{{ entity.CampusCarreraid }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('evaluacion') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('evaluacion_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
