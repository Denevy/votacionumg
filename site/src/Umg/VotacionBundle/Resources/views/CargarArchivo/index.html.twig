{% extends '::base.html.twig' %}
{% block titulo %}
{% endblock %}
{% block body %}
<div class="row">
  <div class="col-md-12">
    <div class="panel panel-primary">
      <div class="panel-heading">
          <h1 class="page-title"> Cargar Estudiantes </h1>
          <h3>Cargar Archivo </h3>
      </div>
      <div class="panel-body">
        <form role="form" method="post" enctype="multipart/form-data" action='{{path('showarchivo')}}'>
          <div class="input-group">
            <label class="sr-only">Archivo</label>
            <input type="file" name="archivo" class="filestyle" data-buttonName="btn-primary" data-placeholder="No ha seleccionado" data-buttonText="Busca Archivo"/>
            <p class="help-block">Formatos adminitidos (.xls .xlsx .ods .csv .xlt)</p>
          </div>
          <button type="submit" class="btn btn-danger fa fa-refresh icon-upload-alt"> Subir</button>
        </form>
        <hr/>
      </div>
    </div>
  </div>
</div>  
{% endblock %}
