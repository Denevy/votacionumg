{% extends '::base.html.twig' %}
{% block titulo %}
    <h1 class="page-title">Evaluaci√≥n docente</h1>
    <p class="lead">{{cc.PensumAnio.Codigo}} - {{cc.PensumAnio.Curso}}<br />{% for cat in cc.catedraticoCursos%}{{cat}}{%endfor%}</p>
{% endblock %}

{% block body -%}

<table class="table table-responsive">
    {% set id = 0 %}
    {% set tipo = 0 %}
    {% for resp in respuestas %}
        <tr>
            <th width="50%">
                {% if resp.Observacion %}
                    Observaciones
                {%else%}
                    {{resp.Preguntum.Pregunta}}
                {%endif%}
            <td> {{resp.Respuesta}} </td>
        </tr>
    {% endfor %}
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('ver_resultado',{'id':evaluacion.id}) }}">
            Regresar a la lista
        </a>
    </li>
</ul>
{% endblock %}
